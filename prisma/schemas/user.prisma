model User {
  id          String   @id @default(cuid())
  name        String   @unique
  email       String   @unique
  password    String
  avatarUrl   String?  @map("avatar_url")
  phoneNumber String?  @default("") @map("phone_number")
  createdAt   DateTime @default(now()) @map("created_at")
  updatedAt   DateTime @updatedAt @map("updated_at")
}
